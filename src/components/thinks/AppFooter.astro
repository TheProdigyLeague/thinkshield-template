---
import "primeflex/primeflex.css";
import "primeicons/primeicons.css";
import { Button } from "primereact/button";
import { InputText } from "primereact/inputtext";
import "primereact/resources/primereact.min.css";
import "./app-footer.css";
import { InputTextarea } from "primereact/inputtextarea";
---

<div id="test" class="bg-white col-12">
  <div class="flex flex-column px-4 py-4 md:px-4 lg:px-8">
    <div class="col-12 grid grid-nogutter text-center lg:text-left">
      <div class="lg:col-12 sm:col-12 md:col-12 xl:col-4 lg:col-3 mt-2">
        <div
          class="transition-duration-150 mt-3 block text-300 text-black-alpha-80 block mt-4 mr-3"
        >
          <div
            class="flex col-12 flex-column p-0 m-0 justify-content-center align-items-center text-green-500"
          >
            <img
              src="src/assets/images/logoHere.jpg"
              alt="thinks logo"
              style="width:160px"
            />
            <p class="text-2xl text-gray-600">Caring for Community</p>
          </div>

          <div class="text-900 text-gray-700 block mt-4 mr-3">
            For further information on how <span class="text-900 text-green-500"
              >ThinkShield</span
            > can support you or your organisation please contact:
            <div class="mt-5">
              <div>
                <span class="text-900 text-green-500">ThinkVitals</span> is the brand
                name of <span class="text-900 text-green-500"
                  >ThinkShield Ltd.</span
                >
              </div>
              <div>
                <span class="text-900 text-green-500">ThinkShield</span> is registered
                in England and Wales Company number: 5161513.
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="sx:col-12 sm:col-12 md:col-12 lg:col-12 xl:col-8">
        <div class="grid col-12">
          <div
            class="xl:col-4 lg:col-6 md:col-6 sm:col-12 sx:col-12 text-center"
          >
            <span class="text-900 text-5xl text-black-alpha-80"> About us</span>
            <ul class="list-none p-0">
              <li>
                <a
                  class="hover:text-900 transition-duration-150 cursor-pointer mt-3 block"
                >
                  Our services
                </a>
              </li>
              <li>
                <a
                  class="hover:text-900 transition-duration-150 cursor-pointer mt-3 block"
                >
                  Our partners
                </a>
              </li>
              <li>
                <a
                  class="hover:text-900 transition-duration-150 cursor-pointer mt-3 block"
                >
                  Our privacy Policies
                </a>
              </li>
            </ul>
          </div>
          <div class="xl:col-4 lg:col-6 md:col-6 sm:col-12 sx:col-12">
            <span class="text-900 text-5xl text-black-alpha-80">Products</span>
            <ul class="list-none p-0">
              <li>
                <a
                  class="hover:text-900 transition-duration-150 cursor-pointer mt-3 block"
                >
                  ThinkShield Ltd.
                </a>
              </li>
              <li>
                <a
                  class="hover:text-900 transition-duration-150 cursor-pointer mt-3 block"
                >
                  ThinkVitals ED.
                </a>
              </li>
            </ul>
          </div>
          <div class="xl:col-4 lg:col-6 md:col-6 sm:col-12 sx:col-12">
            <span class="text-900 text-5xl text-black-alpha-80">Contact us</span
            >
            <ul class="list-none p-0">
              <li>
                <a
                  class="pi pi-home hover:text-900 transition-duration-150 cursor-pointer mt-3 block text-900 text-orange-500"
                >
                  ThinkShield Ltd.</a
                >
              </li>
              <li>
                <a
                  class="pi pi-home hover:text-900 transition-duration-150 cursor-pointer mt-3 block text-900 text-orange-500"
                >
                  ThinkShield India Pvt. Ltd.</a
                >
              </li>
              <li>
                <a
                  class="pi pi-inbox hover:text-900 transition-duration-150 cursor-pointer mt-3 block"
                >
                  Accounts@thinkshield.com
                </a>
              </li>
            </ul>
          </div>
        </div>

        <div class="grid lg:col-12 sm:col-12 md:col-12 xl:col-12">
          <div class="col-12">
            <ul class="grid list-none p-2">
              <li class="xl:col-4 lg:col-4 md:col-6 sm:col-12 sx:col-12 col-12">
                <InputText
                  className="col-12 border-green-500 bg-green-50 border-round-lg"
                  placeholder="Email"
                />
              </li>
              <li class="xl:col-4 lg:col-4 md:col-6 sm:col-12 sx:col-12 col-12">
                <InputText
                  className="col-12 border-green-500 bg-green-50 border-round-lg"
                  placeholder="Organization"
                />
              </li>
              <li class="xl:col-4 lg:col-4 md:col-6 sm:col-12 sx:col-12 col-12">
                <InputText
                  className="col-12 border-green-500 bg-green-50 border-round-lg"
                  placeholder="Full name"
                />
              </li>
              <li class="xl:col-4 lg:col-4 md:col-6 sm:col-12 sx:col-12 col-12">
                <InputText
                  className="col-12 border-green-500 bg-green-50 border-round-lg"
                  placeholder="Telephone"
                />
              </li>
              <li class="xl:col-4 lg:col-4 md:col-6 sm:col-12 sx:col-12 col-12">
                <InputText
                  className="col-12 border-green-500 bg-green-50 border-round-lg"
                  placeholder="Postal address"
                />
              </li>
              <li class="xl:col-4 lg:col-4 md:col-6 sm:col-12 sx:col-12 col-12">
                <InputText
                  className="col-12 border-green-500 bg-green-50 border-round-lg"
                  placeholder="Subject"
                />
              </li>
              <li class="col-12">
                <InputTextarea
                  autoResize
                  className="col-12 h-5rem border-green-500 bg-green-50 border-round-lg"
                  placeholder="Message"
                />
              </li>
              <li class="col-12">
                <Button
                  className="col-12 bg-green-400 border-green-400 hover:bg-green-500"
                  label="Submit"
                />
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="col-12 text-gray-500 text-center">
